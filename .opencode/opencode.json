{
  "$schema": "https://opencode.ai/config.json",
  "agent": {
    "build": {
      "description": "Primary development agent with full codebase access",
      "model": "openai/gpt-5.3-codex"
    },
    "compaction": {
      "description": "Session summarizer for context continuity across compactions",
      "model": "github-copilot/gpt-5.2-codex"
    },
    "explore": {
      "description": "Fast codebase search specialist",
      "model": "github-copilot/gemini-3.1-pro-preview"
    },
    "general": {
      "description": "General-purpose subagent for fast, well-defined tasks; delegates complexity quickly",
      "model": "github-copilot/gpt-5.2-codex"
    },
    "looker": {
      "description": "Media extraction specialist for images, PDFs, diagrams",
      "model": "github-copilot/claude-sonnet-4.6"
    },
    "painter": {
      "description": "Image generation and editing specialist using Gemini 3 Pro Image. Use for creating UI mockups, app icons, hero images, visual assets, and editing/redacting existing images",
      "model": "github-copilot/gemini-3.1-pro-preview"
    },
    "plan": {
      "description": "Primary planning agent for architecture and multi-phase execution",
      "model": "github-copilot/claude-opus-4.6"
    },
    "review": {
      "description": "Code review, debugging, and security audit specialist",
      "model": "github-copilot/gpt-5.2-codex"
    },
    "scout": {
      "description": "External research specialist for library docs and patterns",
      "model": "github-copilot/claude-opus-4.6"
    },
    "vision": {
      "description": "Visual content specialist for multimodal analysis and UI/UX guidance",
      "model": "github-copilot/gemini-3.1-pro-preview"
    }
  },
  "autoupdate": false,
  "formatter": {
    "biome": {
      "command": [
        "npx",
        "@biomejs/biome",
        "check",
        "--write",
        "$FILE"
      ],
      "extensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".json",
        ".jsonc"
      ]
    },
    "cargo-fmt": {
      "command": [
        "cargo",
        "fmt",
        "--",
        "$FILE"
      ],
      "extensions": [
        ".rs"
      ]
    },
    "java-formatter": {
      "command": [
        "google-java-format",
        "--replace",
        "$FILE"
      ],
      "environment": {
        "JAVA_HOME": "{env:JAVA_HOME}"
      },
      "extensions": [
        ".java"
      ]
    },
    "laravel-pint": {
      "command": [
        "npx",
        "laravel-pint",
        "--preset",
        "psr12",
        "$FILE"
      ],
      "extensions": [
        ".php"
      ]
    },
    "oxfmt": {
      "command": [
        "npx",
        "oxfmt",
        "--write",
        "$FILE"
      ],
      "extensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".json",
        ".jsonc",
        ".html",
        ".css",
        ".scss",
        ".sass",
        ".md",
        ".mdx",
        ".yaml",
        ".yml"
      ]
    }
  },
  "instructions": [
    ".opencode/memory/project/user.md",
    ".opencode/memory/project/tech-stack.md",
    ".opencode/memory/project/project.md",
    ".opencode/memory/project/roadmap.md",
    ".opencode/memory/project/state.md"
  ],
  "keybinds": {
    "command_list": ";",
    "leader": "`",
    "session_child_cycle": "ctrl+alt+right",
    "session_child_cycle_reverse": "ctrl+alt+left",
    "session_compact": "ctrl+k"
  },
  "mcp": {
    "stitch": {
      "enabled": true,
      "headers": {
        "Authorization": "Bearer {env:STITCH_ACCESS_TOKEN}",
        "X-Goog-User-Project": "{env:GOOGLE_CLOUD_PROJECT}"
      },
      "oauth": false,
      "type": "remote",
      "url": "https://stitch.googleapis.com/mcp"
    }
  },
  "model": "opencode/kimi-k2.5-free",
  "permission": {
    "bash": {
      "*": "allow",
      "cat *": "allow",
      "git branch *": "allow",
      "git checkout *": "ask",
      "git clean *": "ask",
      "git commit *": "ask",
      "git diff *": "allow",
      "git log *": "allow",
      "git merge *": "ask",
      "git push *": "ask",
      "git rebase *": "ask",
      "git reset *": "ask",
      "git status *": "allow",
      "ls *": "allow",
      "npm *": "allow",
      "npm publish *": "ask",
      "npm run db:push*": "ask",
      "npm run db:reset*": "deny",
      "npx *": "allow",
      "rm *": "deny",
      "rm -rf *": "deny",
      "sudo *": "deny"
    },
    "doom_loop": "ask",
    "edit": "allow",
    "external_directory": "ask",
    "question": "allow",
    "read": {
      "*": "allow",
      "*.env": "deny",
      "*.env.*": "deny",
      "*.env.example": "allow",
      "*.key": "deny",
      "*.pem": "deny",
      "*credentials*": "deny",
      "*password*": "deny",
      "*secret*": "deny",
      "*token*": "deny"
    }
  },
  "plugin": [
    "@tarquinen/opencode-dcp@latest",
    "@franlol/opencode-md-table-formatter@0.0.3"
  ],
  "provider": {
    "modal": {
